{
	"name": "ts-react-frontend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "parcel serve public/index.html --open -d build --port 8000",
		"build": "parcel build public/index.html -d build",
		"test": "jest",
		"test:coverage": "yarn test -- --watchAll false --coverage",
		"lint": "tslint -c tslint.json 'src/**/*.ts*'",
		"format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
		"validate": "npm-run-all --parallel lint test:coverage build"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"husky": "^2.3.0",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.3",
		"prettier": "^1.17.1",
		"pretty-quick": "^1.10.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1"
	},
	"dependencies": {
		"@types/jest": "^24.0.13",
		"@types/react": "^16.8.17",
		"@types/react-dom": "^16.8.4",
		"jest": "^24.8.0",
		"jest-dom": "^3.2.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-icons": "^3.7.0",
		"react-test-renderer": "^16.8.6",
		"react-testing-library": "^7.0.0",
		"ts-jest": "^24.0.2",
		"tslint": "^5.16.0",
		"tslint-react": "^4.0.0",
		"typescript": "^3.4.5"
